
# Auth0 (for code exchange)
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=
AUTH0_CALLBACK_URL=http://localhost:8000/api/v1/auth/callback # Or wherever your backend callback is, if needed directly

# Session JWT
JWT_SECRET_KEY=generate_a_strong_random_32_byte_secret_here # Replace with output of: openssl rand -hex 32
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
